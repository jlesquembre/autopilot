{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start_old": "electron main.js",
    "build": "cross-env HOT=0 NODE_ENV=production webpack --config webpack.config.prod.js",
    "build2": "cross-env HOT=0 NODE_ENV=production webpack --config webpack.config.prod.js && electron-packager . autopilot --platform=linux --arch=x64 --overwrite --ignore=node_modules",
    "server": "node devServer.js",
    "electron": "cross-env HOT=1 NODE_ENV=development electron ./",
    "dev": "concurrently --kill-others --prefix command \"npm run server\" \"npm run electron\""
  },
  "author": "",
  "license": "GPLv2",
  "dependencies": {
    "@cycle/core": "^6.0.0",
    "@cycle/dom": "^9.4.0",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cycle-restart": "^0.0.14",
    "debug-menu": "^0.4.0",
    "electron-contextmenu-middleware": "^1.0.1",
    "electron-debug": "^1.0.1",
    "electron-input-menu": "^2.0.3",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.2.5",
    "eslint": "^2.8.0",
    "eslint-plugin-babel": "^3.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "normalize.css": "^4.0.0",
    "postcss": "^5.0.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.3.0",
    "postcss-loader": "^0.9.1",
    "postcss-modules-values": "^1.1.2",
    "postcss-reporter": "^1.3.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.2",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "webpack-validator": "^2.2.2"
  }
